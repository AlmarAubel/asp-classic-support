{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2019",
		"lib": ["ES2019"],
		"outDir": "out",
		"resolveJsonModule": true,
		"rootDir": "src",
		"sourceMap": true,
		"esModuleInterop": true,
		"alwaysStrict": true,
		"removeComments": true
	},
	"include": ["src"],
	"exclude": ["node_modules", ".vscode-test"]
}
